{
  "name": "interactive-logger",
  "version": "1.0.1",
  "description": "In-browser logger with persistent session storage and downloadable logs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "keywords": [
    "logger",
    "browser",
    "debug",
    "session",
    "download",
    "interactive"
  ],
  "author": "Nishant",
  "license": "CC-BY-NC-4.0",
  "dependencies": {
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "lz-string": "^1.5.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@vitest/ui": "^2.0.0",
    "fake-indexeddb": "^5.0.2",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  }
}
